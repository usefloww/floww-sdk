{
  "name": "floww",
  "version": "0.0.33",
  "description": "SDK for building trigger-based workflows",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usefloww/floww.git",
    "directory": "packages/sdk"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "floww": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js",
      "require": "./dist/ai/index.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js",
      "require": "./dist/runtime/index.cjs"
    },
    "./kv": {
      "types": "./dist/kv/index.d.ts",
      "import": "./dist/kv/index.js",
      "require": "./dist/kv/index.cjs"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.cjs"
    },
    "./providers/server": {
      "types": "./dist/providers/server/index.d.ts",
      "import": "./dist/providers/server/index.js",
      "require": "./dist/providers/server/index.cjs"
    },
    "./providers/constants": {
      "types": "./dist/providers/constants/index.d.ts",
      "import": "./dist/providers/constants/index.js",
      "require": "./dist/providers/constants/index.cjs"
    },
    "./codeExecution": {
      "types": "./dist/codeExecution/index.d.ts",
      "import": "./dist/codeExecution/index.js",
      "require": "./dist/codeExecution/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm typecheck && tsup",
    "dev": "tsup --watch",
    "floww": "tsx cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run",
    "prepublishOnly": "pnpm build",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "keywords": [
    "trigger",
    "workflow",
    "automation",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.42",
    "@ai-sdk/google": "^2.0.29",
    "@ai-sdk/openai": "^2.0.64",
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "@lydell/node-pty": "^1.1.0",
    "@slack/web-api": "^7.12.0",
    "@types/inquirer": "^9.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "ai": "6.0.0-beta.88",
    "centrifuge": "^5.5.2",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "consola": "^3.4.2",
    "execa": "^9.6.0",
    "fastify": "^5.6.1",
    "ink": "^6.4.0",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "log-symbols": "^7.0.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "open": "^10.2.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@floww/api-contract": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "@types/node-cron": "^3.0.11",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "bugs": {
    "url": "https://github.com/usefloww/floww/issues"
  },
  "homepage": "https://github.com/usefloww/floww/tree/main/packages/sdk#readme"
}
